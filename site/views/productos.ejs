<!DOCTYPE html>
<html lang="es">
  <%- include('./partials/head',{title: 'Tienda Argenta'}) %>
<body class="productos">
  <%- include('./partials/header') %>
    <main>
        <h1 class="productsPage"><%= categorias.nombre %></h1>
        <section class="cardProduct">
          <% productoPorCategoria.productos.forEach(producto => { %>
              <%- include('./partials/cards',{
                  id: producto.id,
                  imagen: producto.imagen.nombre,
                  titulo: producto.nombre,
                  descuento: producto.descuento,
                  precio: producto.precio
              }) %> 
          <% }) %>
        </section>
    </main>
    <%- include('./partials/footer') %>
</body>
</html>